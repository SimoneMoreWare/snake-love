{
  "dependencies": {
    "dayjs": "^1.11.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "eslint": "^9.23.0",
    "globals": "^16.0.0",
    "htmlhint": "^1.1.4",
    "husky": "^8.0.0",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "stylelint": "^16.17.0",
    "stylelint-config-recommended": "^15.0.0",
    "stylelint-config-standard": "^37.0.0",
    "stylelint-config-standard-scss": "^14.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.mjs": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.css": [
      "stylelint --fix"
    ]
  },
  "scripts": {
    "prepare": "husky install"
  }
}
